# GeoQuest Backend Environment Variables
# Copy this file to .env and configure with your values

# WebSocket Server Port
# The port on which the backend server will listen
PORT=5001

# CORS Origin
# The frontend origin allowed to connect to this server
# For local development: http://localhost:5000
# For Docker development: http://frontend:5000
CORS_ORIGIN=http://localhost:5000

# Node Environment
# development | production | test
NODE_ENV=development
